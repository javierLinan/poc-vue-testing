    "@vue/eslint-config-prettier": "~6.0.0",
    "@vue/eslint-config-typescript": "~7.0.0",
    "eslint": "~6.7.2",
    "eslint-plugin-prettier": "~3.3.1",
    "eslint-plugin-vue": "~6.2.2",
    "jest": "~26.6.3",
    "jest-puppeteer": "~4.4.0",
    "prettier": "~2.2.1",
    "puppeteer": "~8.0.0",
    "typescript": "~4.4.3",
    "vue-template-compiler": "~2.6.11"
