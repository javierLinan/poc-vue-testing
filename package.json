    "vue-router": "~3.2.0"
  },
  "devDependencies": {
    "@types/expect-puppeteer": "^4.4.6",
    "@types/jest": "~27.0.1",
    "@types/jest-environment-puppeteer": "~4.4.1",
    "@types/puppeteer": "~5.4.4",
    "@types/node": "~15.14.1",
    "@typescript-eslint/eslint-plugin": "~4.18.0",
    "@typescript-eslint/parser": "~4.18.0",
